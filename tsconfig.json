{
  "compilerOptions": {
    "experimentalDecorators": true, 
    "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "strictPropertyInitialization": false,
    "noEmit": true
  }
}
